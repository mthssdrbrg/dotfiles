XDG_CONFIG_HOME DEFAULT=@{HOME}/.config
XDG_DATA_HOME   DEFAULT=@{HOME}/.local/share
XDG_CACHE_HOME  DEFAULT=@{HOME}/.cache
DEV_HOME        DEFAULT=@{HOME}/dev
DEV_BIN         DEFAULT=${DEV_HOME}/bin
DEV_SRC         DEFAULT=${DEV_HOME}/src
DEV_PKG         DEFAULT=${DEV_HOME}/pkg
RCRC            DEFAULT=@{HOME}/.rcrc
EDITOR          DEFAULT=nvim
VISUAL          DEFAULT=nvim
BROWSER         DEFAULT=google-chrome-stable
TERMINAL        DEFAULT=alacritty
LANG            DEFAULT=en_US.UTF-8
LANGUAGE        DEFAULT=${LANG}
LC_CTYPE        DEFAULT=${LANG}
LC_ALL          DEFAULT=${LANG}
LC_COLLATE      DEFAULT=${LANG}
LC_CTYPE        DEFAULT=${LANG}
LC_MESSAGES     DEFAULT=${LANG}
LC_MONETARY     DEFAULT=${LANG}
LC_NUMERIC      DEFAULT=${LANG}
LC_TIME         DEFAULT=${LANG}
LC_ALL          DEFAULT=${LANG}
LESSCHARSET     DEFAULT=utf-8
NO_AT_BRIDGE    DEFAULT=1
CM_LAUNCHER     DEFAULT=rofi
